            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/aus-eng-comp-std> ;
            skos:definition """Comprehensive, theory based understanding of the underpinning natural and physical sciences and the engineering fundamentals applicable to the engineering discipline."""@en ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/aus-eng-comp-std> ;
            skos:altLabel "1.1 Comprehensive, theory based understanding"@en ;
