        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/aus-eng-comp-std> ;
        skos:definition """Knowledge and Skills Base refers to the group of competencies that represent an engineering professional's knowledge of engineering principles and practices."""@en ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/aus-eng-comp-std> ;
        skos:altLabel "1. Knowledge and Skills Base"@en ;
        skos:prefLabel "Knowledge and Skills Base"@en ;
        skos:broader aecs:stage-1 .

