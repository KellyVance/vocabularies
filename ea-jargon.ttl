        skos:broader eaj:identity-management, eaj:validation ;
        skos:definition """Identity validation is the first level of identity confirmation in which an individual's personal information, such as their name, address, phone number and email address, are checked to validate that they exist in the real world."""@en ;
        skos:prefLabel "Identity Validation"@en .

    eaj:identity-verification a skos:Concept ;
        dcterms:source "https://www.experian.co.uk/blogs/latest-thinking/fraud-prevention/what-is-the-difference-between-validation-verification-and-authentication/"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:broader eaj:identity-management, eaj:verification ;
        skos:definition """Identity verification is the second level of identity confirmation after validation in which an individual is linked directly to identity data, and verified as genuine through additional checks."""@en ;
        skos:prefLabel "Identity Verification"@en .

    eaj:identity-authentication a skos:Concept ;
        dcterms:source "https://www.experian.co.uk/blogs/latest-thinking/fraud-prevention/what-is-the-difference-between-validation-verification-and-authentication/"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:broader eaj:identity-management ;
        skos:definition """Authentication is the process of checking user identity against information that only the user should have, or know. To check they are who they say they are."""@en ;
        skos:altLabel "Identity Authentication"@en ;
        skos:prefLabel "Authentication"@en .

    eaj:identity-authorisation a skos:Concept ;
        dcterms:source "https://www.experian.co.uk/blogs/latest-thinking/fraud-prevention/what-is-the-difference-between-validation-verification-and-authentication/"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:broader eaj:identity-management ;
        skos:definition """Authorisation is the process of granting a user access to resources (systems, platforms, or data) based on their identity, roles, and permissions."""@en ;
        skos:altLabel "Identity Authorisation"@en,
            "Identity Authorization"@en,
            "Authorization"@en ;
        skos:prefLabel "Authorisation"@en .

eaj:identity-management-app a skos:Concept ;
    dcterms:source "https://www.vmware.com/topics/glossary/content/identity-management.html"^^xsd:anyURI ;
    rdfs:seeAlso eaj:identity-management ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """An identity management system prevents unauthorized access to systems and resources, helps prevent exfiltration of enterprise or protected data, and raises alerts and alarms when access attempts are made by unauthorized personnel or programs, whether from inside or outside the enterprise perimeter."""@en ;
    skos:altLabel "Identity and Access Management Application"@en, 
        "Identity Management System"@en,
        "Identity Management Tool"@en;
    skos:prefLabel "Identity Management Application"@en .

eaj:information a skos:Concept ;
    dcterms:source "https://www.diffen.com/difference/Data_vs_Information"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """When data is processed, organized, structured or presented in a given context so as to make it useful, it is called information.
    
    In practice, information management deals primarily with unstructured and semi-structured data and includes information, records, and document management in an organisation, whereas data management typically deals with the structured data stored in databases and other tabular formats."""@en ;
    skos:prefLabel "Information"@en .

eaj:issue a skos:Concept ;
    dcterms:source "https://wiki.en.it-processmaps.com/index.php/ITIL_Glossary/_ITIL_Terms_P#Problem"^^xsd:anyURI ;
	rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon>;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
	skos:definition """A cause of one or more Incidents. The cause is not usually known at the time a Problem Record is created."""@en;
    skos:altLabel "Problem"@en ;
    skos:prefLabel "Issue"@en .

eaj:knowledgebase a skos:Concept ;
    dcterms:source "https://www.atlassian.com/itsm/knowledge-management/what-is-a-knowledge-base/"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """A knowledge base is a self-serve online library of information about a product, service, department, or topic. The data in your knowledge base can come from anywhere. Typically, contributors who are well versed in the relevant subjects add to and expand the knowledge base."""@en ;
    skos:notation "KB" ;
    skos:prefLabel "Knowledgebase"@en .

eaj:least-privilege a skos:Concept ;
    dcterms:source "https://www.okta.com/identity-101/minimum-access-policy/"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """A minimum access policy restricts a user to only the least amount of access to privileged resources and permissions that are needed to perform an authorized activity or activities, such as those necessary for employees to do their jobs. This is a cybersecurity practice that can help to protect critical assets and sensitive data. This is also known as the Principle of Least Privilege.
    
    The minimum access policy allows a process or a user account to have only the privileges that are necessary to perform their intended function. Typically, this will mean setting the least amount of access as the default and only opening up permissions and privileges to essential resources and actions. User accounts should run and launch applications with the minimum number of privileges possible."""@en ;
    skos:altLabel "Minimium Access Policy"@en,
        "Minimum Access"@en,
        "Principle of Least Privilege" ;
    skos:prefLabel "Least Privilege"@en .

eaj:likelihood a skos:Concept ;
    dcterms:source "https://www.iso.org/standard/65694.html"^^xsd:anyURI ;
	rdfs:isDefinedBy <https://linked.data.gov.au/def/event-probability> ;
	rdfs:seeAlso <https://linked.data.gov.au/def/event-probability>;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
	skos:definition """Chance of something happening"""@en;
    skos:altLabel "Event Likelihood"@en ;
    skos:prefLabel "Likelihood"@en .

eaj:master-data-management a skos:Concept ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:related eaj:master-data ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """Master data management (MDM) focuses on creating and maintaining accurate, consistent, and reliable master data across different systems and applications. It is a technology-enabled discipline that ensures the uniformity, accuracy, stewardship, semantic consistency and accountability of the enterprise's official shared master data assets. Where Master Data refers to a set of core non-transactional data elements that are shared across multiple applications or business processes within an organization, Master Data Management controls which instance of master data is considered the mastered version and source-of-truth to be referred to or (where essential) replicated from and synchronised with."""@en ;
    skos:notation "MDM" ;
    skos:prefLabel "Master Data Management"@en .

eaj:membership-change a skos:Concept ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
    skos:related <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """The changes possible to membership status between non-members, active members, and past members intersecting with paid vs unpaid memberships withspecial considerations for students and honorary fellows. Membership churn risk also augmented with specific classes based on payment status in the context of member retention and erasure."""@en ;
    skos:prefLabel "Engineers Australia Membership Changes"@en .

eaj:membership-grade a skos:Concept ;
    dcterms:source "https://www.engineersaustralia.org.au/membership/member"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-membership-grade> ;
    skos:related <https://ref.engineersaustralia.org.au/def/ea-membership-grade> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """The grade of membership of Engineers Australia. Grade reflects the experience and skill level of a member and corresponds to a specific level of EA Membership.
    The grades are:
        - Student
        - Graduate
        - Member
        - Fellow
        - Affiliate
        - Companion
        - Honorary Fellow."""@en ;
    skos:prefLabel "Engineers Australia Membership Grade"@en .

eaj:mfa a skos:Concept ;
    dcterms:source "https://www.cyber.gov.au/protect-yourself/securing-your-accounts/multi-factor-authentication"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/australian-qualifications-framework> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """Multi-factor authentication (MFA) is a security measure that requires two or more proofs of identity to grant you access. Multi-factor authentication typically requires a combination of something the user knows (PIN, secret question), something you have (card, token) or something you are (fingerprint or other biometric)."""@en ;
    skos:notation "MFA" ;
    skos:altLabel "MFA"@en ;
    skos:prefLabel "Multi-factor authentication"@en .

eaj:monitoring a skos:Concept ;
    dcterms:source "https://www.iso.org/standard/65694.html"^^xsd:anyURI ;
	rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon>;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
	skos:definition """Continual checking, supervising, critically observing or determining the status in order to identify change from the performance level required or expected."""@en;
    skos:prefLabel "Monitoring"@en .

eaj:ner a skos:Concept ;
    dcterms:source "https://www.engineersaustralia.org.au/credentials/registration/national-engineering-register"^^xsd:anyURI ;
    rdfs:seeAlso <https://ref.engineersaustralia.org.au/def/areasofpractice/areas-of-practice-ner> ;
	rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon>;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
	skos:definition """Engineers Australia introduced the National Engineering Register (NER) in 2015. This comprehensive directory is a way to recognise engineers who meet the high standards of skill, qualification and professionalism expected in the industry.
    
    It provides a national framework of registration for professional engineers, engineering technologists and engineering associates across the private and public sectors.
    
    As a publicly-searchable database, the NER gives individuals and businesses access to a qualified pool of engineers, endorsed by Australia's engineering peak body."""@en;
    skos:notation "NER" ;
    skos:prefLabel "National Engineering Register"@en .

eaj:occupational-categories a skos:Concept ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/iea-occupational-categories> ;
    skos:related <https://linked.data.gov.au/def/iea-occupational-categories> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """A category reflecting the level and type of engineering qualification held by a member of the engineering profession, as per the International Engineering Alliance Educational Accords. This include Engineering Associate, Engineering Technologist, and Professional Engineer."""@en ;
    skos:altLabel "International Engineering Accords Membership"@en,
        "Occupational Categories"@en ;
    skos:prefLabel "International Engineering Alliance Occupational Categories"@en .

    eaj:mra-occupational-categories a skos:Concept ;
        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:related <https://linked.data.gov.au/def/iea-occupational-categories> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:broader eaj:occupational-categories ;
        skos:definition """An MRA Occupational category is a translation between a specific MRA Occupational Category and the equivalent Engineers Australia Occupational Category."""@en ;
        skos:prefLabel "MRA Occupational Categories"@en .

eaj:offering a skos:Concept ;
    dcterms:source "https://opentext.wsu.edu/marketing/chapter/8-1-what-composes-an-offering/"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """Offerings are products and services designed to deliver value to customers. Either to fulfill their needs, satisfy their wants, or both. Offerings are anything that delivers value to known stakeholders, users, or customers. This may be a service, a physical product, or an abstract product such as a qualification or title."""@en ;
    skos:scopeNote """Offerings are equivalent to the Scrum and Agile term Product."""@en ;
    skos:prefLabel "Offering"@en .

    eaj:product a skos:Concept ;
        dcterms:source "https://chisellabs.com/glossary/what-is-a-product-offering/"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:definition """A product offering refers to an item that you sell to customers, such as a tangible physical product or software, or it may refer to an intangible title, status, or registration such authorised recognition as a qualified professional."""@en ;
        skos:scopeNote """In Scrum and Agile terminology a Product encompasses both Products and Services and is therefore conceptually equivalent to the general concept of an Offering."""@en ;
        skos:prefLabel "Product"@en ;
        skos:broader eaj:offering .

        eaj:product-physical a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:broader eaj:product ;
            skos:definition """A tangible output that provides value through its access and/or use. Example: Create Magazine."""@en ;
            skos:prefLabel "Physical Product"@en .

        eaj:product-intangible a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:broader eaj:product ;
            skos:definition """An intangible output that provides value through its ownership or association. Example: Memberships, Titles, Status, Registration, Usage Rights."""@en ;
            skos:prefLabel "Intangible Product"@en .

            eaj:product-digital a skos:Concept ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
                skos:broader eaj:product-intangible ;
                skos:definition """A software output that provides value through its access and/or use. Typically made available through one or more underlying technologies, other digital products or data products. Example: EEA Online Dashboard; EEA Online Course (e.g., Systems Thinking in Engineering)."""@en ;
                skos:prefLabel "Digital Product"@en .

            eaj:product-data a skos:Concept ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
                skos:broader eaj:product-intangible ;
                skos:definition """A self-contained output that answers a business question; may be directly consumed or bundled in relevant digital products or services; are typically made available through one or more underlying technologies. Example: EA People Pulse Survey Summary (Results)."""@en ;
                skos:scopeNote """At EA, consumers of Data Products are primarily internal to the organisation."""@en ;
                skos:prefLabel "Data Product"@en .

    eaj:service a skos:Concept ;
        dcterms:source "https://chisellabs.com/glossary/what-is-a-product-offering/"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
        skos:broader eaj:offering ;
        skos:definition """A service offering involves providing intangible services or expertise to customers, such as consulting, maintenance, or professional services. Services are processes and activities undertaken on behalf of a customer."""@en ;
        skos:prefLabel "Service"@en .

        eaj:service-high-touch a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:broader eaj:service ;
            skos:definition """Provides value to consumers through assisted access (e.g., via Business Processes) to resources and information. May bundle relevant physical products, digital products, data products and/or other services. Example: EngInsure Claim Lodgement; Engineer Registration; Chartered Assessment."""@en ;
            skos:prefLabel "High-Touch Service"@en .

        eaj:service-low-touch a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
            skos:broader eaj:service ;
            skos:definition """Like a High-Touch Service, except that a consumer may access resources and information without assistance (e.g., via a given Role or Business Process); usually via a digital channel (e.g., portal/web page/app). Example: Meeting Room Booking via Outlook 365."""@en ;
            skos:altLabel "Self-Service"@en ;
            skos:prefLabel "Low-Touch Service"@en .

eaj:pcas a skos:Concept ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """A scheme managed by the Engineers Australia for professional engineers (you must be on the NPER Register to be an Accredited Certifier) who want to be accredited as Private Certifiers under the Environmental Planning and Assessment ACT (NSW) 1979 (the EPA Act). Please contact the Registration & Practice Team for more information about the scheme. This scheme was transferred to the NSW State Government Building Professionals Board on 1 March 2007."""@en ;
    skos:notation "PCAS" ;
    skos:prefLabel "Private Certifier Accreditation Scheme"@en .

eaj:personal-information a skos:Concept ;
    dcterms:source "https://www.oaic.gov.au/privacy/privacy-guidance-for-organisations-and-government-agencies/handling-personal-information/what-is-personal-information"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """Information or an opinion about an identified individual, or an individual who is reasonably identifiable:
    whether the information or opinion is true or not; and
    whether the information or opinion is recorded in a material form or not."""@en ;
    skos:prefLabel "Personal Information"@en .

eaj:personally-identifiable-information a skos:Concept ;
    dcterms:source "https://www.oaic.gov.au/privacy/privacy-guidance-for-organisations-and-government-agencies/handling-personal-information/what-is-personal-information"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:related <https://linked.data.gov.au/def/identifying-information> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """Personally identifiable information (PII) refers to details that can be used to confirm who you are or how to find you. Where personal information includes all details about a person who can be identified, PII is the specific information that can be used to make an identity connection.
    
    Personally identifiable information is any piece of information or data that can help to confirm who an individual is or how to find them. It may be identifying information on its own (direct identification) or in context with other information (indirect identification)."""@en ;
    skos:notation "PII" ;
    skos:prefLabel "Personally Identifiable Information"@en .

eaj:persistent-identifier a skos:Concept ;
    dcterms:source "https://github.com/geological-survey-of-queensland/persistent-identifiers/"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """An identifier (ID) is any label used to uniquely name a thing (person, place, event), whether online or offline. URLs are an example of an identifier, as are serial numbers or personal names. A persistent identifier (PID) is an identifier that is guaranteed to be managed and kept up to date to reliably point to an individual or unique digital or physical entity."""@en ;
    skos:notation "PID" ;
    skos:altLabel "Global Unique Identifier"@en ;
    skos:prefLabel "Persistent Identifier"@en .

eaj:post-nominals a skos:Concept ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    dcterms:references <https://en.wikipedia.org/wiki/List_of_post-nominal_letters_(Australia)>,
        <https://www.stylemanual.gov.au/grammar-punctuation-and-conventions/titles-honours-forms-address/awards-and-honours>,
        <https://www.pmc.gov.au/honours-and-symbols/australian-honours-system/post-nominals> ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-jargon> ;
    skos:definition """Letters and notations placed following a person's last name to indicate education qualifications, title of office, decoration or honour. Postnominals include abbreviations of an award or awarding institution. 
    
    Post nominals follow an order of preference as per the following simplified heirarchy:
    - post-nominals for honours and awards
    - state appointments of King's Counsel (KC) or Senior Counsel (SC) or Justice of the Peace (JP)
    - university degrees and diplomas
    - membership of professional associations
    - membership of parliament
