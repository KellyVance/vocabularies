eaj:ewc a skos:Concept ;
    dcterms:source "https://www.engineersaustralia.org.au/employers"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-membership-agreements> ;
