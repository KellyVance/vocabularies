eamc:past-member a skos:Concept ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
    skos:definition """An individual whose membership with Engineers Australia is inactive."""@en ;
    skos:altLabel "Non-Current Member"@en ;
    skos:prefLabel "Past Member"@en .

    eamc:inactive a skos:Concept ;
        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
        skos:broader eamc:past-member ;
        skos:definition """An individual whose membership with Engineers Australia is inactive, and did not hold membership with Engineers Australia at any time during the reference period."""@en ;
        skos:prefLabel "Inactive"@en .

    eamc:removal a skos:Concept ;
        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
        skos:broader eamc:past-member ;
        skos:definition """An individual whose membership with Engineers Australia is inactive, but who held membership with Engineers Australia during the reference period."""@en ;
        skos:altLabel "Lost Member"@en ;
        skos:prefLabel "Removal"@en .

        eamc:student-loss a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
            skos:broader eamc:removal ;
            skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Free (Student) membership with Engineers Australia during the reference period."""@en ;
            skos:note """Student loss is most commonly due to the maximum duration for student membership being exceeding. This duration may be extended at the discretion of Engineers Australia under extenuating circumstances."""@en ;
            skos:altLabel "Student Erasure"@en ;
            skos:prefLabel "Student Loss"@en .

        eamc:member-loss a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
            skos:broader eamc:removal ;
            skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period."""@en ;
            skos:prefLabel "Member Loss"@en .

            eamc:termination a skos:Concept ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                skos:broader eamc:member-loss;
                skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended independently of an Engineers Australia decision."""@en ;
                skos:prefLabel "Termination"@en  .

                eamc:resignation a skos:Concept ;
                    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                    skos:broader eamc:termination ;
                    skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended through a resignation and deliberate non-payment of fees."""@en ;
                    skos:note "Resignation is an active decision by the customer to resign their membership and to inform EA of intent not to renew their membership, whereas Non-Renewal is a passive non-payment of membership fees."@en ;
                    skos:prefLabel "Resignation"@en .

                eamc:deceased a skos:Concept ;
                    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                    skos:broader eamc:termination ;
                    skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended due to the death of the individual."""@en ;
                    skos:prefLabel "Deceased"@en .

            eamc:cancellation a skos:Concept ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                skos:broader eamc:member-loss ;
                skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended through a decision by Engineers Australia."""@en ;
                skos:prefLabel "Cancellaion"@en .

                eamc:non-renewal a skos:Concept ;
                    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                    skos:broader eamc:cancellation ;
                    skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended through a decision by Engineers Australia due to a non-payment of fees and subsequent termination of membership."""@en ;
                    skos:note "Non-Renewal is a passive non-payment of membership fees, whereas resignation is an active decision by the customer to resign their membership and to inform EA of intent not to renew their membership."@en ;
                    skos:altLabel "Erasure"@en,
