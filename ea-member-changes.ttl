                        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                        skos:broader eamc:revocation ;
                        skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended for a defined period through a decision by Engineers Australia due to discretionary reasons outside non-payment of membership fees."""@en ;
                        skos:prefLabel "Suspension"@en .

                    eamc:expulsion a skos:Concept ;
                        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-member-changes> ;
                        skos:broader eamc:revocation ;
                        skos:definition """An individual whose membership with Engineers Australia is inactive, but who held a Paid membership with Engineers Australia during the reference period, and where membership was ended indefinitely or permanently through a decision by Engineers Australia due to discretionary reasons outside non-payment of membership fees."""@en ;
                        skos:prefLabel "Expelled"@en .

