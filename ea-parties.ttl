prty:person a skos:Concept ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-party> ;
    skos:definition """A person is an individual human, acting in a personal or professional capacity. A natural human person."""@en ;
    skos:scopeNote "An individual acting in a business capacity as a Sole Trader is considered an organisation that is a separate entity from the individual as a person."@en ;
    skos:altLabel "Individual"@en,
        "Contact"@en ;
    skos:prefLabel "Person"@en .

prty:organisation a skos:Concept ;
    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
    skos:topConceptOf <https://ref.engineersaustralia.org.au/def/ea-party> ;
    skos:definition """An organisation is a group of people or other entities that interact together to achieve a common goal or perform an action."""@en ;
    skos:notation "Org" ;
    skos:altLabel "Organization"@en,
        "Account"@en ;
    skos:prefLabel "Organisation"@en .

    prty:internal-group a skos:Concept ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
        skos:broader prty:organisation ;
        skos:definition """An internal group is a group of people or other entities within, and intrinsically part of, an organisation that work together to achieve a specific goal or set of goals."""@en ;
        skos:altLabel "Internal Organisation"@en ;
        skos:prefLabel "Internal Group"@en .

        prty:business-unit a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """A business unit is a department, division, or other functional entity within an organization that is responsible for a specific business function or set of functions."""@en ;
            skos:notation "BU" ;
            skos:altLabel "Group"@en ;
            skos:prefLabel "Business Unit"@en .

            prty:division a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:business-unit ;
                skos:definition """A Division is an Engineers Australia Office that serves the EA Members & engineering community in a specific geographical region."""@en ;
                skos:notation "Div" ;
                skos:altLabel "State Division"@en;
                skos:prefLabel "Division"@en .

            prty:department a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:business-unit ;
                skos:definition """A department is a functional business unit that performs a specific function or set of functions within an organization."""@en ;
                skos:notation "FBU" ;
                skos:altLabel "Functional Unit"@en,
                    "Team"@en,
                    "Functional Business Unit"@en ;
                skos:prefLabel "Department"@en .

            prty:cost-centre a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:business-unit ;
                skos:definition """An operating business unit used for the management and operational control of business processes that span legal entites. Cost Centres are used to accumulate and manage costs, budgets, and expenditures. Cost centres are typically related to other business units and functions where one cost centre may be related to functions across many other business units, or a single business unit may be related to multiple cost centres."""@en ;
                skos:prefLabel "Cost Centre"@en .

        prty:board a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """A group of people that are elected to represent the stakeholders in an organisation."""@en ;
            skos:altLabel "The Board"@en ;
            skos:prefLabel "Board"@en .

            prty:joint-board a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:board ;
                skos:definition """A group of people that are elected to represent the stakeholders across the organisations within a joint venture."""@en ;
                skos:prefLabel "Joint Board"@en .

            prty:project-control-board a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://www.linkedin.com/advice/0/how-do-you-select-train-project-control-board-members"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:board ;
                skos:definition """Project control board (PCB) is a key element of project governance that provides oversight, direction, and decision-making for complex and high-risk projects."""@en ;
                skos:notation "PCB" ;
                skos:prefLabel "Project Control Board"@en .

        prty:committee a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:scopeNote "Committees are groups whose existence is dependent on being part of a larger group. In application, Committees and its narrower concepts should always be linked directly to the larger organisation or group to which they belong."@en ;
            skos:definition """An advisory body to an organisation or group dedicated to a specific function or issue."""@en ;
            skos:prefLabel "Committee"@en .

            prty:board-committee a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:committee ;
                skos:definition """An advisory body to The Board dedicated to a specific function of the organisation."""@en ;
                skos:prefLabel "Board Committee"@en .

            prty:executive-committee a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:committee ;
                skos:definition """An advisory body that is a subset of The Board and supports its functions. Generally consisting of the CEO, senior executives or office bearers, and members elected by The Board. The executive committee may be delegated decision making powerby The Board for urgent decisions and decisions over times when a full board is unavailable or impractical such as during holiday periods."""@en ;
                skos:prefLabel "Executive Committee"@en .

            prty:subcommittee a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:committee ;
                skos:definition """A type of committee that acts as an advisory body to a larger comittee within a dedicated subset of the committee functions. Sub-committees are established by resolution from a committee, with functions specified in the resolution and with a prescribed end date. Subcommittees may not be established as local branch of a college, special interest group or centre, national committee or technical society."""@en ;
                skos:altLabel "Sub Committee"@en,
                    "Subcommittee"@en ;
                skos:prefLabel "Sub-Committee"@en .

                prty:regional-group a skos:Concept ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    skos:broader prty:subcommittee ;
                    skos:definition """A type of sub-committee that acts to support a specified set of functions within a geographical area."""@en ;
                    skos:prefLabel "Regional Group"@en .

        prty:branch a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:scopeNote "Branches are groups whose existence is dependent on being part of a larger group. In application, Branches should always be linked directly to the larger organisation or group to which they belong."@en ;
            skos:definition """A branch is a typically geographically focused sub-division of a broader group. At Engineers Australia branches, refer to local subdivisions of groups that sit outside the organisational structure, such as; Centres, Special Interest Groups, Colleges, National Committee, and Technical Societies. Sub-divisions that represent geographic areas that are part of the organisational structure of Engineers Australia are known as Divisions."""@en ;
            skos:prefLabel "Branch"@en .

        prty:community-of-practice a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """An informal or semi-formal internal group with a shared subject of interest. A forum for exchange of ideas, training, workshops, support, and discussion of related matters."""@en ;
            skos:prefLabel "Community of Practice"@en .

        prty:exec-leadership-team a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:notation "ELT" ;
            skos:altLabel "ELT"@en,
             "ET"@en,
             "Executive Team"@en ;
            skos:definition """Comprised of industry and functional leaders and the CEO, the Executive Team (ET) lead groups across the organisation to deliver on organisational strategy and vision. The ET are responsible for providing strategic and operational leadership. They operate as an interdependent and collaborative decision-making body."""@en ;
            skos:prefLabel "Executive Leadership Team"@en .

        prty:leadership-team a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:notation "LT" ;
            skos:altLabel "LT"@en,
             "Leadership Group"@en ;
            skos:definition """Comprised of functional Heads and General Managers, the Leadership Team (LT) lead teams across the organisation to execute functions and projects in alignment with organisational strategy and vision. The LT are responsible for providing tactical and operational leadership. They operate as an interdependent and collaborative decision-making body."""@en ;
            skos:prefLabel "Leadership Team"@en .

        prty:learned-society a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """A Learned Society is an association of like minded people who develop the knowledge and set the standards of its members. Learned Societies predominantly relate to a profession such as engineering, law, medicine, commerce etc. The EA Learned Society comprises four major groups: College Boards, National Committees, Technical Societies, and Special Interest Groups."""@en ;
            skos:altLabel "EA Learned Society"@en ;
            skos:prefLabel "Learned Society"@en .

            prty:engineers-aus-college a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:learned-society ;
                skos:definition """A College provides a discipline-based home for members and provides a vehicle through which Engineers Australia advances the science and practice of engineering for the benefit of the Community through building the body of engineering knowledge and supporting the career development of engineering professionals."""@en ;
                skos:prefLabel "Engineers Australia College"@en .

            prty:national-committee a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:learned-society,
                    prty:committee ;
                skos:definition """National Committees are consituted by the Engineers Australia Board for issues with a natonal focus. National Committees must include a national focus, a maximum of 6 members, and an appropriate mix of age, gender, location and other aspects of diversity, representation and relevant skills including skills in management; and governance."""@en ;
                skos:prefLabel "National Committee"@en .

            prty:special-interest-group a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:learned-society ;
                skos:definition """A group established by the Board where a national focus is required to ensure that the needs of a specific membership cohort can be addressed. The role of a Special Interest Group is to: 
                (a) identify strategies and recommend to the Board actions to address the needs of their identified cohort;
                (b) work with the Colleges, Technical Societies and the Chief Executive Officer to implement Board approved strategies and policies in relation to their identified cohort into the normal operations of each College, Technical Society and Division, including undertaking joint activities; 
                (d) liaise with and provide support to College Boards, Divisions and Technical Societies regarding the undertaking of activities by these groups to support their cohort; 
                (e) communicate with their cohort to identify ongoing needs and to direct them to areas within Engineers Australia that can assist them; and 
                (f) arrange for an appropriate range of events or other activities in accordance with their approved plan to address needs that cannot normally be met through other Engineers Australia channels."""@en ;
                skos:prefLabel "Special Interest Group"@en .

            prty:special-interest-centre a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:learned-society ;
                skos:definition """A group established by the Board where a national focus is required for a significant area of engineering practice that is not covered by a College and/or Technical Society. The role of a Centre is to: 
                (a) identify strategies and recommend to the Board actions to address the specific needs of their identified area of engineering practice;
                (b) facilitate the provision of high level technical advice, undertake research and/or contribute to public policy and participate in expert panels, committees or forums;
                (c) identify gaps or emerging needs in skills and knowledge for their identified area of engineering practice to facilitate the CPD provided by Engineers Australia for members; and
                (d) represent Engineers Australia in the wider community in line with Engineers Australia's policies, and nominate members for appointment to external bodies as required."""@en ;
                skos:altLabel "Special Interest Centre"@en ;
                skos:prefLabel "Centre"@en .

            prty:technical-society a skos:Concept ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                dcterms:source "https://www.engineersaustralia.org.au/engineering-communities/technical-societies"^^xsd:anyURI ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                skos:broader prty:learned-society ;
                skos:definition """An Engineers Australia Technical Society is a group that facilitates networking between professionals in the same area of expertise, hosts discussion forums, and provides access to technical papers and presentations. Technical societies provide professional technical development opportunities that its members may participate in. Membership in a technical society is not contingent on membership to Engineers Australia."""@en ;
                skos:altLabel "Engineers Australia Technical Society"@en ;
                skos:prefLabel "Technical Society"@en .

                prty:inc-tech-society a skos:Concept ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    dcterms:source "https://www.engineersaustralia.org.au/engineering-communities/technical-societies"^^xsd:anyURI ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    skos:broader prty:technical-society ;
                    skos:definition """Incorporated technical societies are governed by their own constitutions but choose to be aligned with Engineers Australia. Under agreement, these organisations perform the functions of anEngineers Australia technical society, however they remain separate legal entities."""@en ;
                    skos:editorialNote """Incorporated technical societies by strict definition are an external party, however due to the nature of their operation and close interaction with Engineers Australia they are considered part of the EA structure and operation for practical purposes."""@en ;
                    skos:prefLabel "Incorporated Technical Society"@en .

                prty:uninc-tech-society a skos:Concept ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    dcterms:source "https://www.engineersaustralia.org.au/engineering-communities/technical-societies"^^xsd:anyURI ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    skos:broader prty:technical-society ;
                    skos:definition """Unicorporated technical societies are governed by a technical society committee that's under the direction of the Engineers Australia Board and is responsible to it."""@en ;
                    skos:prefLabel "Unincorporated Technical Society"@en .

                prty:aff-tech-society a skos:Concept ;
                    rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
                    skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
                    skos:broader prty:technical-society ;
                    skos:definition """Affiliated technical societies are governed externally and independently from Engineers Australia or via a Joint Board, but have an affiliate relationship. These organisations may be run primarily as an independent Australian Public Company or part thereof and manage their own membership cohort."""@en ;
                    skos:editorialNote """Affiliated technical societies by strict definition are an external party, however due to the nature of their operation and close interaction with Engineers Australia they are considered part of the EA structure and operation for practical purposes."""@en ;
                    skos:prefLabel "Affiliated Technical Society"@en .

        prty:national-congress a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """A National Congress is a primarily advisory body, established so that representatives of Congress can engage directly with the Board to advise them in decision making and strategic priorities. It is made up senior office bearers representing a range of stakeholders and experts across an organisation."""@en ;
            skos:prefLabel "National Congress"@en .

        prty:overseas-chapter a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """Overseas Chapters are representative bodies of a parent organisation consisting of members of the organisation whose principal place of residence is outside the organisation's primary country of operation."""@en ;
            skos:prefLabel "Overseas Chapter"@en .

        prty:subsidiary a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """A subsidiary of the subject organisation."""@en ;
            skos:scopeNote "Subsidiary as a party type should only be used for organisations that are subsidiaries to the organisation whose data is being described, i.e. internal subsidiaries. Where an organisation is a subsidiary to another external organisation it should be categorised as a separate Organisation type with a Subsidiary party relationship to its parent."@en ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:prefLabel "Subsidiary"@en .

        prty:working-group a skos:Concept ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party> ;
            dcterms:source "https://en.wikipedia.org/wiki/Working_group"^^xsd:anyURI ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party> ;
            skos:broader prty:internal-group ;
            skos:definition """A group of experts or representatives within a broader organisation, group, or collective that work together to achieve specified goals. The groups are domain-specific and focus on discussion or activity around a specific subject area. The term can also refer to an interdisciplinary collaboration of researchers or experts working on new activities that would be difficult to sustain under traditional mechanisms."""@en ;
            skos:scopeNote "Working groups may also be partially synonymous with Special Interests Groups, the distinction being that Special Interests Groups must be established by the Board." ;
            skos:altLabel "Working Party"@en ;
            skos:prefLabel "Working Group"@en .

    prty:commercial-entity a skos:Concept ;
