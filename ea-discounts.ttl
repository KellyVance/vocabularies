        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-discounts> ;
        skos:broader disc:add-on ;
        skos:definition """A Graduate Program discount gives individuals who complete the Engineering Education Australia (EEA) graduate program one year of free membership on transition from a Student to a Graduate membership grade."""@en ;
        skos:altLabel "Graduate Year 0"@en ;
        skos:prefLabel "Graduate Program"@en .

