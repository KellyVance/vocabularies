<https://ref.engineersaustralia.org.au/def/ea-membership-grade> a owl:Ontology,
        skos:ConceptScheme ;
    dcterms:accessRights <https://linked.data.gov.au/def/data-access-rights/open> ;
    dcterms:created "2022-11-08"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/engineersaustralia> ;
    dcterms:modified "2024-03-12"^^xsd:date ;
    dcterms:publisher <https://linked.data.gov.au/org/engineersaustralia> ;
    dcterms:source "https://www.engineersaustralia.org.au/membership"^^xsd:anyURI ;
    prov:qualifiedAttribution
        [
            dcat:hadRole earl:data-steward ;
            prov:agent [
                    a prov:Agent ;
                    rdfs:label "Engineers Australia: Member and Customer"
                ]
        ] ;
    skos:definition "The grade of membership at Engineers Australia. Grade reflects the experience and skill level of a member and corresponds to a specific level of EA Membership. The members of Engineers Australia are those persons whose names are entered in the register of members."@en ;
