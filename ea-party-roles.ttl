    earp:employee a skos:Concept ;
        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
        skos:broader earp:contact ;
        skos:altLabel "Staff"@en,
            "Staff Member"@en,
            "Worker"@en ;
        skos:definition """Individual who has signed a contract of employment with an organisation. This may be permanent, fixed term, or casual. An employee is not an individual working for an organisation as part of a third party service or consulting contract. Employees provide their skills and services in exchange for remuneration from their employer.
        
        The inverse of an Employer."""@en ;
        skos:prefLabel "Employee"@en .
        
        earp:exec-leader a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
