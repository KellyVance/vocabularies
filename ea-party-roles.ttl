                skos:broader earp:member ;
                skos:altLabel "Engineers Australia Member"@en ;
                skos:definition """An EA Member is a Person who has (or has had) a membership with Engineers Australia. This is distinct from the term Member when referring to the grade of membership with Engineers Australia."""@en ;
                skos:prefLabel "EA Member"@en .

                earp:ea-corp-member a skos:Concept ;
                    dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
