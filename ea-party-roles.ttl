            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:broader earp:collaborator ;
            skos:altLabel "MRA Organisation"@en ;
            skos:definition """An MRA Organisation is a relationship with an Organisation through a mutual recognition agreement with Engineers Australia and may also offer Chartered Status to Engineers."""@en ;
            skos:prefLabel "Mutual Recognition Agreement Organisation"@en .

    earp:competitor a skos:Concept ;
        dcterms:source "https://learn.microsoft.com/en-us/dynamics365/fin-ops-core/dev-itpro/organization-administration/overview-global-address-book"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
