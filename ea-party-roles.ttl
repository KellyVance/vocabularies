    earp:vendor a skos:Concept ;
        dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
        skos:broader earp:contact ;
        skos:definition """A vendor, also known as a supplier, is a person or an organisation that provides products and services exchanged in a transaction. Vendors interact in the development or delivery of products and services. Vendors may organisations acting as contractors and consultants, companies, tertiary institutions, event venues, government departments or other organisations."""@en ;
        skos:altLabel "Supplier"@en ;
        skos:prefLabel "Vendor"@en .
        
        earp:consultant a skos:Concept ;
            dcterms:source "https://lawpath.com.au/blog/consultant-and-contractor-whats-the-difference"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:broader earp:supplier ;
            skos:definition """A consultant provides the advice and preliminary guidance in accordance with your needs. This can be advice for areas such as strategy, human resources, software adoption or public relations, usually in regards to a specific project. A consultant could give advice for a business owner to work with a contractor, as part of a broader business plan."""@en ;
            skos:prefLabel "Consultant"@en .
    
        earp:contractor a skos:Concept ;
            dcterms:source "https://lawpath.com.au/blog/consultant-and-contractor-whats-the-difference"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:broader earp:supplier ;
            skos:definition """An individual or organisation who is engaged on a fixed contract to provide a service. Contractors are a separate organisation or an individual employed by such an external organisation."""@en ;
            skos:prefLabel "Contractor"@en .

    earp:volunteer a skos:Concept ;
        dcterms:source "https://www.engineersaustralia.org.au/sites/default/files/2024-06/introduction-volunteering-engineers-australia-jun-24.pdf"^^xsd:anyURI ;
        rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
        rdfs:seeAlso <https://ref.engineersaustralia.org.au/def/ea-party-roles/volunteer> ;
        skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
        skos:broader earp:contact ;
        skos:altLabel "EA Volunteer"@en,
            "Engineers Australia Volunteer"@en ;
        skos:definition """Volunteer encompasses all Office Bearers as well as those volunteers who are not members of a College Board or Committee but volunteer their time across other EA business units and activities. Thye are generally short term, task-based, or for a specific project. 
        
        These roles include:
        - Standards Australia Nominees,
        - Technical Journal Editors and Panels,
        - Communities of Practice,
        - Accreditation Panels,
        - IPA Panelists,
        - Awards inc. Judging Panels,
        - Conference Organising Committees,
        - Continuing Professional Development (CPD) Taskforces & Onsite event delivery,
        - Student Ambassadors,
        - Policy and Advocacy Working Groups,
        - Task-Based Teams/Working Groups,
        - Science, Technology, Engineering and Mathematics (STEM) Outreach Volunteers."""@en  
        skos:prefLabel "Volunteer"@en .

        earp:volunteer-sme a skos:Concept ;
            dcterms:source "https://linked.data.gov.au/org/engineersaustralia"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:broader earp:volunteer ;
            skos:definition """Members who bring their expertise and depth of knowledge to assist Engineers Australia in its purpose.

            These include:
            - Speakers at events,
            - Media Spokespeople,
            - Policy and Advocacy Contirbutors."""@en ;
            skos:prefLabel "Volunteer Subject Matter Expert"@en .

        earp:office-bearer a skos:Concept ;
            dcterms:source "https://www.engineersaustralia.org.au/sites/default/files/2023-05/2023-royal-charter-by-laws.pdf"^^xsd:anyURI ;
            rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
            skos:broader earp:volunteer ;
            skos:definition """Any person holding honorary office in Engineers Australia, and includes delegates, directors and members of committees of Engineers Australia (whether members of Engineers Australia or not).
            
            Senior Office Bearer refers to office bearers acting as:
            - Congress Delegates,
            - College Chairs,
            - Division Presidents,
            - Chairs of Standing Committees of the Board,
            - Chairs of College National Committees,
            - Chairs of Joint Boards,
            - Engineer Practice Advisory Committee (EPAC) Members (Chairs/Presidents of Technical Societies, National Committee Chairs, and Chairs of Special Interest Groups/Centres."""@en ;
            skos:prefLabel "Office Bearer"@en .

            earp:appointed-member a skos:Concept ;
                dcterms:source "https://www.engineersaustralia.org.au/sites/default/files/resource-files/2019-05/EA%20General%20Regulations%2002.05.19.pdf"^^xsd:anyURI ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
                skos:broader earp:office-bearer ;
                skos:definition """An office bearer who is appointed to a position of membership in an organsiation, to a board, or to a position by decision of an authorised person."""@en ;
                skos:prefLabel "Appointed Member"@en .

            earp:elected-member a skos:Concept ;
                dcterms:source "https://www.engineersaustralia.org.au/sites/default/files/resource-files/2019-05/EA%20General%20Regulations%2002.05.19.pdf"^^xsd:anyURI ;
                rdfs:isDefinedBy <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
                skos:inScheme <https://ref.engineersaustralia.org.au/def/ea-party-roles> ;
                skos:broader earp:office-bearer ;
                skos:definition """An office bearer who is elected to a position of membership in an organsiation, to a board, or to a position by a vote of electors."""@en ;
                skos:prefLabel "Elected Member"@en .

