    skos:topConceptOf <https://linked.data.gov.au/def/indigenous-status> ;
    skos:definition """When answering the Indigenous Status question 'Yes, Torres Strait Islander' is ticked but 'Yes, Aboriginal' is not ticked."""@en ;
    skos:notation "2" ;
    skos:prefLabel "Torres Strait Islander"@en .

indg:3 a skos:Concept ;
    dcterms:source "https://www.abs.gov.au/statistics/standards/indigenous-status-standard/latest-release"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/indigenous-status> ;
    skos:inScheme <https://linked.data.gov.au/def/indigenous-status> ;
    skos:topConceptOf <https://linked.data.gov.au/def/indigenous-status> ;
    skos:definition """When answering the Indigenous Status question 'Yes, Aboriginal' is ticked and 'Yes, Torres Strait Islander' is also ticked."""@en ;
    skos:notation "3" ;
    skos:prefLabel "Both Aboriginal and Torres Strait Islander origin"@en .

indg:4 a skos:Concept ;
    dcterms:source "https://www.abs.gov.au/statistics/standards/indigenous-status-standard/latest-release"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/indigenous-status> ;
    skos:inScheme <https://linked.data.gov.au/def/indigenous-status> ;
    skos:topConceptOf <https://linked.data.gov.au/def/indigenous-status> ;
    skos:definition """When answering the Indigenous Status question 'No' is ticked."""@en ;
    skos:notation "4" ;
    skos:prefLabel "Neither Aboriginal nor Torres Strait Islander origin"@en .

indg:5 a skos:Concept ;
    dcterms:source "https://www.abs.gov.au/statistics/standards/indigenous-status-standard/latest-release"^^xsd:anyURI ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/indigenous-status> ;
    skos:inScheme <https://linked.data.gov.au/def/indigenous-status> ;
    skos:topConceptOf <https://linked.data.gov.au/def/indigenous-status> ;
    skos:definition """When answering the Indigenous Status question 'No' is ticked and either or both 'Yes, Aboriginal', and 'Yes, Torres Strait Islander' are ticked."""@en ;
    skos:notation "5" ;
    skos:altLabel "Not stated"@en ;
    skos:prefLabel "Prefer not to answer"@en .